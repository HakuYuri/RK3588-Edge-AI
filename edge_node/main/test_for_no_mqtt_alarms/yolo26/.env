# --- 硬件与路径配置 ---
VIDEO_DEV=/dev/video11
RTMP_URL=rtmp://your-srs-server/live/livestream
RKNN_MODEL=test.rknn

# --- 图像尺寸配置 ---
WIDTH=1920
HEIGHT=1080
# 裁切参数 (crop=out_w:out_h:x:y)
CROP_STR=1080:1080:420:0
# 模型输入与输出推流尺寸
INPUT_SIZE=640

# --- 推理与后处理参数 ---
OBJ_THRESH=0.4
NMS_THRESH=0.45
CORE_MASK=1

# --- 编码器配置 ---
BITRATE=4M
FPS=30